# Shared Cloud SQL Instance Credentials
# Copy this file to .env.shared-instance and fill in values
# Run ./export-credentials.sh after deploying to auto-populate

SHARED_SQL_INSTANCE_NAME=tequity-shared-development
SHARED_SQL_CONNECTION_NAME=tequity-ajit:us-central1:tequity-shared-development
SHARED_SQL_IP=<public-ip-address>
SHARED_SQL_ADMIN_USER=postgres
SHARED_SQL_ADMIN_PASSWORD=<generated-password>
MASTER_DATABASE_URL=postgresql://postgres:<password>@<ip>:5432/master_db?sslmode=disable
